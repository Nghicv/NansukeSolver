package nghicv.com.nansukesolver;

public class Variable {
	
	private int col;
	private int row;
	private int index;
	
	public Variable(int col, int row) {
		super();
		this.col = col;
		this.row = row;
	}
	public Variable(int col, int row, int index) {
		super();
		this.col = col;
		this.row = row;
		this.index = index;
	}
	public int getCol() {
		return col;
	}
	public void setCol(int col) {
		this.col = col;
	}
	public int getRow() {
		return row;
	}
	public void setRow(int row) {
		this.row = row;
	}
	public int getIndex() {
		return index;
	}
	public void setIndex(int index) {
		this.index = index;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "("+row+","+col+")"+"/n";
	}
	

}
